function mainvalidate(){
  	if($("#promotionTitle").val()==""){
		alert("活动标题不能为空");
		return false;
	}
	else if($("#promotionStartDate").val()==""){
		    alert("活动开始日期不能为空");
			return false;
		}
	else if($("#promotionEndDate").val()==""){
	    alert("活动结束日期不能为空");
		return false;
	}
	 else if($("#effectivetime").val()!=""&&!/^[0-9]{1,5}$/.test($("#effectivetime").val())){
 			alert("订单支付时间错误");
			return false;
 	} 
    return true;
}