package com.winxuan.ec.admin.controller.message;

/**
 * 订单消息表单
 * @author heyadong
 * @version 1.0, 2013-1-14 下午03:13:21
 */
public class OrderMessageForm {

    //备注
    private String remark;
    //短信类型
    private Long type;
    //渠道
    private Long[] channels;
    //付款类型
    private Long[] paytype;
    //缺货种类需大于
    private Integer kindgreat = 0;
    //缺货种类需小于
    private Integer kindless = Integer.MAX_VALUE;
    //发货类型
    private Long[] deliverytype;
    //消息
    private String message;
   
    public String getRemark() {
        return remark;
    }
    public void setRemark(String remark) {
        this.remark = remark;
    }
    public Long getType() {
        return type;
    }
    public void setType(Long type) {
        this.type = type;
    }
    public Long[] getChannels() {
        return channels;
    }
    public void setChannels(Long[] channels) {
        this.channels = channels;
    }
    public Long[] getPaytype() {
        return paytype;
    }
    public void setPaytype(Long[] paytype) {
        this.paytype = paytype;
    }
    public Integer getKindgreat() {
        return kindgreat;
    }
    public void setKindgreat(Integer kindgreat) {
        this.kindgreat = kindgreat;
    }
    public Integer getKindless() {
        return kindless;
    }
    public void setKindless(Integer kindless) {
        this.kindless = kindless;
    }
    public Long[] getDeliverytype() {
        return deliverytype;
    }
    public void setDeliverytype(Long[] deliverytype) {
        this.deliverytype = deliverytype;
    }
    public String getMessage() {
        return message;
    }
    public void setMessage(String message) {
        this.message = message;
    }
}
