
body,button,input,select,textarea
{
	font: 12px/1.5 tahoma,arial;
}


#rankbar
{
	background: #f3f3f3;
	background: -moz-linear-gradient(50% 0,#fafafa,#e5e5e5);
	background: -webkit-gradient(linear,0 0,0 100%,from(#fafafa),to(#e5e5e5));
	border: 1px solid #ccc;
	border-bottom: 1px solid #ddd;
	height: 28px;
	margin-top: 10px;
	position: relative;
	z-index: 50;
}
#rankbar.sticky
{
	_margin-top: 10px;
	_position: relative;
	box-shadow: 0 1px 5px rgba(0,0,0,0.1);
	margin-top: -1px;
	moz-box-shadow: 0 1px 5px rgba(0,0,0,0.1);
	position: fixed;
	top: 0;
	webkit-box-shadow: 0 1px 5px rgba(0,0,0,0.1);
	z-index: 90000;
}
#rankbar-holder,.sticky-close
{
	display: none;
}
.sticky .sticky-close
{
	display: block;
}
.sticky+#rankbar-holder
{
	display: block;
}
#rankbar-form .old-srp
{
	border-bottom-color: #ccc;
	border-top-color: #ddd;
	margin-top: 0;
}
#rankbar .iw
{
	border: solid #fff;
	border-left: 1px solid #fff;
	border-width: 0 1px;
	height: 28px;
	overflow: hidden;
}
#rankbar h3
{
	line-height: 28px;
	padding: 0 8px;
}
#rankbar h4
{
	float: left;
	font: 100 12px/1 '\5b8b\4f53',serif;
	padding: 8px auto;
}
#rankbar .h-tip
{
	color: #808080;
	padding: 9px 5px 7px 12px;
}
.iw a:hover
{
	color: #404040;
	text-decoration: none;
}
#rankbar .sorting ul
{
	float: left;
}
#rankbar .sorting
{
	float: left;
}
#rankbar .sorting-btns
{
	border-left: 1px solid #cecece;
}
#rankbar .sorting-btns li
{
	float: left;
	position: relative;
}
#rankbar .sorting-btns a
{
	border-left: solid 1px #cecece;
	border-right: solid 1px #cecece;
	display: inline-block;
	margin-left: -1px;
	padding-right: 3px;
}
#rankbar .tgl-switch a:hover,#rankbar .sorting-btns a:link:hover,#rankbar .sorting-btns a:visited:hover
{
	background: -moz-linear-gradient(50% 0,#fff,#ffffd3);
	background: -webkit-gradient(linear,0 0,0 100%,from(#fff),to(#ffffd3));
	background-color: #ffd;
	filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,StartColorStr='#ffffff',EndColorStr='#ffffd3');
	zoom: 1;
}
#rankbar .sorting-btns a span,#rankbar .tgl-switch a span,#rank-sel-sort .toselect a,.sticky-close
{
	background-position: 100% -2094px;
	background-repeat: no-repeat;
}
#rankbar .sorting-btns a span
{
	border-left: solid 1px #fafafa;
	color: #404040;
	display: block;
	line-height: 28px;
	padding: 0 17px 0 7px;
}
#rankbar .sorting-btns a.nodeasc
{
	padding: 0;
}
#rankbar .sorting-btns a.nodeasc span
{
	background-image: none;
	padding-left: 11px;
	padding-right: 11px;
}
#rankbar .sorting-btns .crt
{
	background: #fff;
	left: -1px;
	position: relative;
}
#rankbar .sorting-btns .crt span
{
	padding-bottom: 1px;
}
#rankbar .sorting-btns .dbsort span
{
	background-position: 100% -2029px;
}
#rankbar .sorting-btns .dbsort-asc span
{
	background-position: 100% -2051px;
}
#rankbar .sorting-btns .dbsort-desc span
{
	background-position: 100% -2073px;
}
#rankbar .sorting-btns .sort-asc span
{
	background-position: 100% -2315px;
}
#rankbar .sform
{
	float: right;
	line-height: 1;
	padding: 5px;
}
#rankbar .sform input.txt,#rankbar .sform input[type=text]
{
	height: 13px;
	width: 10em;
}
#rankbar form p
{
	margin: 0;
}
#rankbar input.txt
{
	line-height: 11px;
	padding: 2px;
	vertical-align: middle;
}
#rankbar .btn
{
	vertical-align: middle;
}
#rank-priceform
{
	border-left: 1px solid #fff;
	float: left;
	height: 28px;
	overflow: visible;
	position: relative;
	white-space: nowrap;
	z-index: 2;
}
#rank-priceform .fm-price
{
	padding: 0 9px 0 8px;
	position: absolute;
}
#rank-priceform.focus .fm-price
{
	background: #fff;
	border: 1px solid #ccc;
	box-shadow: 1px 2px 6px rgba(0,0,0,0.2);
	height: auto;
	left: -2px;
	margin-right: -1px;
	moz-box-shadow: 1px 2px 6px rgba(0,0,0,0.2);
	padding-left: 9px;
	top: -1px;
	webkit-box-shadow: 1px 2px 6px rgba(0,0,0,0.2);
}
#rank-priceform p
{
	line-height: 17px;
	margin: 0;
	padding: 4px 0;
}
#rank-priceform div.btns
{
	display: none;
	line-height: 20px;
	padding-bottom: 5px;
	text-align: right;
	z-index: 4;
}
#rank-priceform.focus div.btns
{
	display: block;
}
#rankbar .tgl-switch
{
	_width: 88px;
	height: 19px;
	position: absolute;
	right: 8px;
	top: 5px;
}
#rankbar .tgl-switch a
{
	background: -moz-linear-gradient(50% 0,#fefefe,#f2f2f2);
	background: -webkit-gradient(linear,0 0,0 100%,from(#fefefe),to(#f2f2f2));
	background-color: #f4f4f4;
	border: 1px solid #a6a6a6;
	color: #404040;
	display: block;
	filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,StartColorStr='#fefefe',EndColorStr='#f2f2f2');
	line-height: 17px;
	zoom: 1;
}
#rankbar .tgl-switch a span
{
	background-position: 0 -1998px;
	display: block;
	height: 17px;
	padding: 0 4px 0 20px;
}
#rankbar .tgl-switch-righton a span
{
	background-position: 0 -2014px;
}
