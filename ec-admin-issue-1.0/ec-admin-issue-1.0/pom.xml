<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.winxuan</groupId>
  <artifactId>console</artifactId>
  <packaging>war</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>winxuan-admin Maven Webapp</name>
  <url>http://admin.winxuan.com</url>
  <properties>
  	 <project.build.sourceEncoding> UTF-8 </project.build.sourceEncoding>
  </properties>
  <dependencies>
  	<dependency>
  		<groupId>winxuan</groupId>
  		<artifactId>ec-core</artifactId>
  		<version>1.0.alpha</version>
  		<type>jar</type>
  		<scope>compile</scope>
  		<exclusions>
  			<exclusion>
					<artifactId>guava</artifactId>
					<groupId>guava</groupId>
				</exclusion>
  			<exclusion>
  				<artifactId>hibernate</artifactId>
  				<groupId>org.hibernate</groupId>
  			</exclusion>
  			 <exclusion>
  				<artifactId>poi</artifactId>
  				<groupId>poi</groupId>
  			</exclusion> 
  			 <exclusion>
  				<artifactId>commons-digester</artifactId>
  				<groupId>commons-digester</groupId>
  			</exclusion> 
  		</exclusions>
  		
  	</dependency>
  	<dependency>
  		<groupId>jexcelapi</groupId>
  		<artifactId>jxl</artifactId>
  		<version>2.6</version>
  		<scope>compile</scope>
  	</dependency>
  	<dependency>
  		<groupId>com.oracle</groupId>
  		<artifactId>ojdbc14</artifactId>
  		<version>20110505</version>
  		<type>jar</type>
  		<scope>compile</scope>
  	</dependency>
	<dependency>
    	<groupId>org.apache.poi</groupId>
    	<artifactId>poi-ooxml</artifactId>
    	<version>3.6</version>
	</dependency>
  	<dependency>
  		<groupId>com.winxuan</groupId>
  		<artifactId>api-control</artifactId>
  		<version>1.0.RELEASE</version>
  		<type>jar</type>
  		<scope>compile</scope>
  	</dependency>
  	<dependency>
  		<groupId>commons-digester</groupId>
  		<artifactId>commons-digester</artifactId>
  		<version>1.8</version>
  		<type>jar</type>
  		<scope>compile</scope>
  	</dependency>
	<dependency>
			<groupId>com.winxuan</groupId>
			<artifactId>guava</artifactId>
			<version>16.0.1</version>
		</dependency>
  	<dependency>
  		<groupId>com.winxuan</groupId>
  		<artifactId>wx-heartbeat</artifactId>
  		<version>1.0-SNAPSHOT</version>
  		<type>jar</type>
  		<scope>compile</scope>
  	</dependency>
  	<dependency>
  		<groupId>org.mockito</groupId>
  		<artifactId>mockito-all</artifactId>
  		<version>1.9.5</version>
  		<scope>test</scope>
  	</dependency>
		<dependency>
			<groupId>com.winxuan</groupId>
			<artifactId>dip.interface</artifactId>
			<version>0.1.0-alpha</version>
		</dependency>
  </dependencies> 
  <build>
   <resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
			<resource>
				<directory>src/main/java</directory>
				<includes>
					<include>**/*.xml</include>	
				</includes>
			</resource>
		</resources>
  <plugins>
		  <plugin>	  
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
          <configuration> 
          <encoding>UTF-8</encoding>
         </configuration>
        <executions>
          <execution>
            <id>default-testCompile</id>
            <phase>test-compile</phase>
            <goals>
              <goal>testCompile</goal>
            </goals>
          </execution>
          <execution>
            <id>default-compile</id>
            <phase>compile</phase>
            <goals>
              <goal>compile</goal>
            </goals>
          </execution>
        </executions>
		  </plugin>
		</plugins>
    <finalName>console</finalName> 
  </build>
</project>
