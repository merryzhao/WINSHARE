package com.winxuan.ec.front.controller.customer;


/**
 * 用户基本信息
 * 
 * @author YangJun
 * @version 1.0,2011-10-18
 */
public class MoreForm {
	private String livingStatus;
	private Long careerType;
	private Long career;
	private Long salary;
	private String interest;
	private String favorite;
	private short flag = 1;// 默认为查询

	public String getLivingStatus() {
		return livingStatus;
	}

	public void setLivingStatus(String livingStatus) {
		this.livingStatus = livingStatus;
	}

	public Long getCareerType() {
		return careerType;
	}

	public void setCareerType(Long careerType) {
		this.careerType = careerType;
	}

	public Long getCareer() {
		return career;
	}

	public void setCareer(Long career) {
		this.career = career;
	}

	public Long getSalary() {
		return salary;
	}

	public void setSalary(Long salary) {
		this.salary = salary;
	}

	public String getInterest() {
		return interest;
	}

	public void setInterest(String interest) {
		this.interest = interest;
	}

	public String getFavorite() {
		return favorite;
	}

	public void setFavorite(String favorite) {
		this.favorite = favorite;
	}

	public short getFlag() {
		return flag;
	}

	public void setFlag(short flag) {
		this.flag = flag;
	}

}
