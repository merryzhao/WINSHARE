<%@page pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<div fragment="${fragment.id}" class="index_right_fixheight">
<jsp:include page="../null.jsp"/>
	<c:forEach items="${contentList}" var="content">
	<p class="margin10"><a   target="_blank"  href="${content.url}" title="${content.title}"><img width="200" alt="${content.title}" src="${content.src}"></a></p>
	</c:forEach>
</div>