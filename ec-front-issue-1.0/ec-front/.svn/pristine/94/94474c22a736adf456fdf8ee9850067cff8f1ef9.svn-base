@charset "utf-8";
/* CSS Document */
#help_r{margin:10px auto;}
#help_r a{ color:#003399; text-decoration:none;}
#help_r a:hover{ color:#003399; text-decoration: underline;}
#help_r .table{background-color:#efefef!important;}
#help_r .table_td{background-color:#fafafa!important;}
#help_r .table_td2{background-color:#ffffff!important;}
#help_r .table2{background-color: #efefef!important;}
#help_r .table2_td{background-color: #fafafa!important;}
#help_r .table2_td2{background-color:#ffffff!important;}
#help_r .worm_word{padding:10px; margin-left:10px; border:1px solid #F7DABD; background-color:#FEFCED; font-size:12px; line-height:20px;}
#help_r .{font-size:14px; line-height:30px;}
#help_r .tel{color: #CC0033; line-height:20px;}
#help_r .fontbold{font-weight:bold;}
#help_r h3{margin:0px auto; padding-left:10px; height:28px; line-height:28px; font-size:14px; font-weight:bold; background-color: #F7F7F7; border-bottom:1px solid #e0e0e0;}
#help_r dl{ margin:0px; line-height:22px;}
#help_r dl h4{ margin:8px 0px 4px 0px; font-size:12px; line-height:30px; padding-left:10px;  font-weight:bold; background-color:#FAFAFA;}
#help_r dl h5{ margin:4px 0px; font-weight:bold; }
#help_r dt { margin-top:5px; margin-bottom:5px; padding-left:10px; line-height:30px;}
#help_r dd {margin:0px;line-height:30px; padding-left:10px;}
#help_r .dot{ padding-left:24px; background: url(../images/dot.png)  no-repeat 8px center;}
#help_r dt .moneybg{width:750px; height:487px; position:relative; background:url(../images/money.jpg) no-repeat;}
#help_r	dt .moneybg p{position:absolute; bottom:5px; left:25px; font-size:12px; color:#5f5c4c;}
#help_r dt .warning{font-weight:700; font-size:14px;}
#help_r dt .limg{vertical-align: middle;}	